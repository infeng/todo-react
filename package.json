{
  "name": "todo-react",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index",
  "scripts": {
    "start": "dora --port 8001 --plugins \"webpack,webpack-hmr,browser-history?index=/demo/index.html\"",
    "lint": "tslint -c tslint.json src/**/*.ts src/**/*.tsx",
    "build": "export ENV=production && atool-build",
    "lib": "rm -rf tslib && tsc --outDir tslib",
    "test": "jest"
  },
  "keywords": [
    "react",
    "typescript"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]    
  },
  "author": "",
  "homepage": "",
  "dependencies": {
    "@types/jest": "^19.2.3",
    "@types/node": "^6.0.45",
    "@types/react": "^15.0.20",
    "@types/react-dom": "^0.14.23",
    "@types/redux-actions": "^1.2.4",
    "atool-build": "^0.8.1",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "dora": "^0.4.3",
    "dora-plugin-browser-history": "^0.2.0",
    "dora-plugin-webpack": "^0.8.1",
    "dora-plugin-webpack-hmr": "^0.2.1",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.3",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.5",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "style-loader": "^0.16.1",
    "ts-jest": "^20.0.4",
    "tslint": "^3.15.1",
    "typescript": "^2.1.4",
    "webpack": "^1.13.2"
  }
}
